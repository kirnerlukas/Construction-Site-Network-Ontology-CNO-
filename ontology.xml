    <!-- htto://w3id.org/omg#hasGeometry -->


    <owl:ObjectProperty rdf:about="htto://w3id.org/omg#hasGeometry">
        <rdfs:domain rdf:resource="http://w3id.org/cno#Obstacle"/>
        <rdfs:range rdf:resource="https://w3id.org/omg#Geometry"/>
        <rdfs:comment>A spatial representation for a given Feature.</rdfs:comment>
